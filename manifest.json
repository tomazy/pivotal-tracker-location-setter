{
  "manifest_version": 2,
  "name": "PivotalTracker Location Setter",
  "version": "0.0.0.1",
  "description": "Sets time shift location to 'Singapore'",
  "content_scripts": [
    {
      "matches": [
        "*://www.pivotaltracker.com/time_shifts/*"
      ],
      "js": [
        "pivotalTrackerLocationSetter.js"
      ]
    }
  ]
}
